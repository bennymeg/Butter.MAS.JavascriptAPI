{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "standard"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "semi": "off",
        "indent": "off",
        "space-before-function-paren": "off",
        "no-useless-constructor": "off",
        "no-multiple-empty-lines": "off",
        "no-unreachable": "off",
        "no-unused-vars": "off"
    }
}
